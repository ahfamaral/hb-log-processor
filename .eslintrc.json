{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "semi": [2, "never"],
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-console": "off"
    }
}